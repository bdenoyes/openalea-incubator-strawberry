    "     \n",
    "    \"\"\"\n",
    "\n",
    "    cpx = g.complex(vid)\n",
    "    nxid = g.node(cpx) \n",
    "    if nxid.visible:\n",
    "        if g.label(vid) == 'HT':\n",
    "            return 7\n",
    "        else:\n",
    "            return 6\n",
    "    \n",
    "    # select s, ht, et bt\n",
    "    for cid in nxid.components():\n",
    "        label = cid.label\n",
    "        if label in ('s', 'ht', 'bt'):\n",
    "            if label == 's':\n",
    "                return 1\n",
    "            elif label == 'bt':\n",
    "                stage = cid.Stade\n",
    "                if stage in (None, '17', '18', '19'):\n",
    "                    return 2\n",
    "                elif stage == 'A':\n",
    "                    return 3\n",
    "                elif stage in ('pourri', 'aborted', 'dried'):\n",
    "                    return 4\n",
    "            elif label  == 'ht':\n",
    "                return 5\n",
